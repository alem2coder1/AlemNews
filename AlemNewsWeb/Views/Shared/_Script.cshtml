@switch (CurrentLanguage)
{
    case "en":
        {
            <script src="/console/js/plugins/uppy/langs/en_US.min.js" asp-append-version="true"></script>
        }
        break;
    case "zh-cn":
        {
            <script src="/console/js/plugins/uppy/langs/zh_CN.min.js" asp-append-version="true"></script>
        }
        break;
    case "ru":
        {
            <script src="/console/js/plugins/uppy/langs/ru_RU.min.js" asp-append-version="true"></script>
        }
        break;
    case "tr":
        {
            <script src="/console/js/plugins/uppy/langs/tr_TR.min.js" asp-append-version="true"></script>
        }
        break;
    case "tote":
        {
            <script src="/console/js/plugins/uppy/langs/tote_KZ.min.js" asp-append-version="true"></script>
        }
        break;
    case "latyn":
        {
            <script src="/console/js/plugins/uppy/langs/latyn_KZ.min.js" asp-append-version="true"></script>
        }
        break;
    default:
        {
            <script src="/console/js/plugins/uppy/langs/kk_KZ.min.js" asp-append-version="true"></script>
        }
        break;
}

<script src="~/console/js/common.js" asp-append-version="true"></script>
<script src="~/console/js/main.js" asp-append-version="true"></script>