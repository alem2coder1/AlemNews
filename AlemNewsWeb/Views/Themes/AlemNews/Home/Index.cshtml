                <!--Item slider-->
               @foreach(var pinned in pinnedArticle)
               {
                   <article class="col-12">
                                    <div class="card card-full text-light overflow zoom">
                                      <!--thumbnail-->
                                      <div class="height-ratio image-wrapper">
                                        <a href="/@CurrentLanguage/article/@(pinned.LatynUrl).html">
                                          <img class="img-fluid lazy last-img-height" src="@GetUrl(pinned?.ThumbnailUrl.Replace("_big.", "_middle."))"
                                            data-src="@GetUrl(pinned?.ThumbnailUrl.Replace("_big.", "_middle."))" alt="@pinned?.Title">
                                        </a>
                                        <div class="position-absolute p-3 b-0 w-100 bg-lg-shadow">
