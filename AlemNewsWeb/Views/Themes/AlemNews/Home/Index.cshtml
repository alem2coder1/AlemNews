                                          <a href="/@CurrentLanguage/article/@(pinned.LatynUrl).html">
                                            <h2
                                              class="h1-sm h2-md display-4-lg fw-bold text-capitalize heading-letter-spacing text-white">
                                              @pinned?.Title</h2>
                                          </a>
                                          <!-- meta title -->
                                          <div class="news-meta">
                                            <time class="news-date" datetime="2019-10-22">@UnixTimeHelper.UnixTimeToLocalString(pinned.AddTime,CurrentLanguage)</time>
