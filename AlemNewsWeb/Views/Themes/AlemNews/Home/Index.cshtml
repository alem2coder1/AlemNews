                            <h2 class="h6 h4-sm h6-md h5-lg text-white my-1 news-title">@focusArticleList[j]?.Title
                            </h2>
                          </a>
                        </div>
                      </div>
                    </div>
                  </article>
                }


              </div>
            }
          
